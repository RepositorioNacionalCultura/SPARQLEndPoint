    <owl:DatatypeProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2004/02/skos/core#definition"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#prefLabel -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2004/02/skos/core#prefLabel"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#scopeNote -->

    <owl:DatatypeProperty rdf:about="http://www.w3.org/2004/02/skos/core#scopeNote"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dbpedia.org/ontology/TimePeriod -->

    <owl:Class rdf:about="http://dbpedia.org/ontology/TimePeriod">
        <owl:equivalentClass rdf:resource="http://mdm/ontologies/2017/9/onto#Periodo_temporal"/>
        <rdfs:isDefinedBy>http://dbpedia.org/ontology/TimePeriod</rdfs:isDefinedBy>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Cosa_inmaterial -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Cosa_inmaterial">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E70_Cosa"/>
        <rdfs:comment xml:lang="es">Cosa que no tiene naturaleza física.</rdfs:comment>
        <rdfs:label xml:lang="es">Cosa inmaterial</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Cosa_inmaterial_creada_por_humanos -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Cosa_inmaterial_creada_por_humanos">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Cosa_inmaterial"/>
        <rdfs:comment xml:lang="es">Cosa de carácter intelectual generada como fruto de una actividad llevada a cabo por humanos.</rdfs:comment>
        <rdfs:label xml:lang="es">Cosa inmaterial creada por humanos</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Descripcion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Descripcion">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E33_Objeto_linguistico"/>
        <rdfs:comment xml:lang="es">Elemento que contiene una descripción libre de las características físicas y el contexto histórico y cultural de los objetos culturales.</rdfs:comment>
        <rdfs:label xml:lang="es">Descripción</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Descriptor -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Descriptor">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E89_Objeto_proposicional"/>
        <rdfs:comment xml:lang="es">Esta clase incluye aquellos conceptos utilizados típicamente por un catalogador para describir el contenido o aspectos específicos de un objeto de información.</rdfs:comment>
        <rdfs:label xml:lang="es">Descriptor</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E11_Modificacion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E11_Modificacion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E11_Modification"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E7_Actividad"/>
        <rdfs:comment xml:lang="es">Aquí se incluyen actividades de creación o modificación como las restauraciones.</rdfs:comment>
        <rdfs:label xml:lang="es">Modificación</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E12_Proceso_Produccion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E12_Proceso_Produccion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E12_Production"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E11_Modificacion"/>
        <rdfs:comment xml:lang="es">Esta clase comprende las actividades diseñadas para la producción exitosa de uno o más objetos, artefactos o construcciones.</rdfs:comment>
        <rdfs:label xml:lang="es">Proceso de producción</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E18_Cosa_física -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E18_Cosa_física">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E18_Physical_Thing"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E70_Cosa"/>
        <rdfs:comment xml:lang="es">Esta clase comprende todos los elementos físicos persistentes (ya sean naturales o fabricados por humanos) que tienen una forma relativamente estable.</rdfs:comment>
        <rdfs:label xml:lang="es">Cosa física</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E1_Entidad -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E1_Entidad">
        <rdfs:comment xml:lang="es">Esta clase comprende todas las entidades que se definen en el Modelo de datos México. Equivalente a E1_CRM_Entity.</rdfs:comment>
        <rdfs:label xml:lang="es">Entidad</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E21_Persona -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E21_Persona">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E21_Person"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E39_Agente"/>
        <rdfs:comment xml:lang="es">Incluye a personas reales vivas o que se presume que han vivido. Las figuras legendarias pueden considerarse en esta categoría si la documentación se refiere a ellas como figuras históricas.</rdfs:comment>
        <rdfs:label xml:lang="es">Persona</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E22_Objeto_cultural -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E22_Objeto_cultural">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E22_Man-Made_Object"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E24_Cosa_fisica_creada_por_humanos"/>
        <rdfs:comment xml:lang="es">Esta clase comprende objetos culturales físicos creados de manera consciente por humanos. Incluye bienes muebles e inmuebles. Equivale a E22_Man-Made_Object. Es subclase exclusivamente de &quot;Objeto físico creado por humanos&quot;.</rdfs:comment>
        <rdfs:label xml:lang="es">Objeto cultural</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E24_Cosa_fisica_creada_por_humanos -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E24_Cosa_fisica_creada_por_humanos">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E24_Physical_Man-Made_Thing"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E18_Cosa_física"/>
        <rdfs:comment xml:lang="es">Esta clase incluye todos las cosas físicas individuales o agrupadas que han sido creadas de manera intencionada como producto de una actividad humana. Equivale a E24_Physical_Man-Made_Thing.</rdfs:comment>
        <rdfs:label xml:lang="es">Cosa física creada por humanos</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E26_Caracteristica_fisica -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E26_Caracteristica_fisica">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E26_Physical_Feature"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E18_Cosa_física"/>
        <rdfs:comment xml:lang="es">Características físicas identificables en un objeto físico (como rayaduras, hoyos o manchas de humedad), independientemente de su origen. Equivale a E26_Physical_Feature.</rdfs:comment>
        <rdfs:label xml:lang="es">Característica física</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E28_Objeto_Conceptual -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E28_Objeto_Conceptual">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E28_Conceptual_Object"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Cosa_inmaterial_creada_por_humanos"/>
        <rdfs:comment xml:lang="es">Comprende productos inmateriales cuya generación puede haber estado asistida por dispositivos tecnológicos. Pueden existir simultáneamente en más de un soporte (papel, pinturas, fotos, formato digital, audio, etc.).</rdfs:comment>
        <rdfs:label xml:lang="es">Objeto conceptual</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E2_Entidad_Efimera -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E2_Entidad_Efimera">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E2_Temporal_Entity"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E1_Entidad"/>
        <rdfs:comment xml:lang="es">Clase que recoge entidades (periodos, eventos o estados) que tienen una duración limitada en el tiempo.</rdfs:comment>
        <rdfs:label xml:lang="es">Entidad efímera</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E30_Derechos -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E30_Derechos">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E30_Right"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E89_Objeto_proposicional"/>
        <rdfs:comment xml:lang="es">Esta clase recoge los privilegios legales que tienen que ver con cosas materiales, inmateriales o sus derivados. Incluye los derechos de autor o los derechos de reproducción. Su valor puede ser una texto o una URL que enlace un documento con el texto íntegro de la licencia.</rdfs:comment>
        <rdfs:label xml:lang="es">Derechos</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E31_Documento -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E31_Documento">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E31_Document"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E73_Objeto_de_Informacion"/>
        <rdfs:comment xml:lang="es">Comprende objetos inmateriales que hacen proposiciones sobre la realidad en forma de texto, imágenes, gráficos, audiogramas, videogramas, etc. Un caso específico son las bases de datos.</rdfs:comment>
        <rdfs:label xml:lang="es">Documento</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E33_Objeto_linguistico -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E33_Objeto_linguistico">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E33_Linguistic_Object"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E73_Objeto_de_Informacion"/>
        <rdfs:comment xml:lang="es">Esta clase recoge expresiones en lenguaje natural que hacen proposiciones sobre un objeto cultural en un determinado idioma.</rdfs:comment>
        <rdfs:label xml:lang="es">Objeto lingüístico</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E34_Inscripcion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E34_Inscripcion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E34_Inscription"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E33_Objeto_linguistico"/>
        <rdfs:comment xml:lang="es">Esta clase comprende pequeños textos escritos sobre objetos físicos creados por humanos. No estarían incluídas las marcas como las firmas autógrafas.</rdfs:comment>
        <rdfs:label xml:lang="es">Inscripción</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E35_Titulo -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E35_Titulo">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E35_Title"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E33_Objeto_linguistico"/>
        <rdfs:comment xml:lang="es">Incluye los nombres asignados a obras de cualquier índole, como textos, pinturas, esculturas o piezas musicales.</rdfs:comment>
        <rdfs:label xml:lang="es">Título</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E36_Objeto_visual -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E36_Objeto_visual">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E36_Visual_Item"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E73_Objeto_de_Informacion"/>
        <rdfs:comment xml:lang="es">Comprende los aspectos intelectuales y conceptuales de un objeto de caracter visual como signos, marcas e imágenes.</rdfs:comment>
        <rdfs:label xml:lang="es">Objeto visual</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E37_Marca -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E37_Marca">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E37_Mark"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E36_Objeto_visual"/>
        <rdfs:comment xml:lang="es">En esta clase están incluidas aquellas marcas o símbolos que se encuentran sobre un objeto cultural con una semántica determinada como, por ejemplo, la firma autógrafa de un autor. Quedan fuera de la cobertura de esta clase las marcas que son consideradas desperfectos que afectan al estado de conservación de la pieza.</rdfs:comment>
        <rdfs:label xml:lang="es">Marca</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E38_Imagen -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E38_Imagen">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E38_Image"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E36_Objeto_visual"/>
        <rdfs:comment xml:lang="es">Distribuciones de formas y color en cualquier tipo de soporte.</rdfs:comment>
        <rdfs:label xml:lang="es">Imagen</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E39_Agente -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E39_Agente">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E39_Actor"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E77_Elemento_persistente"/>
        <rdfs:comment xml:lang="es">Esta clase incluye personas individuales o en grupo con capacidad de realizar acciones intencionales de las cuales alguien se puede hacer responsable.</rdfs:comment>
        <rdfs:label xml:lang="es">Agente</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E40_Institucion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E40_Institucion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E40_Legal_Body"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E39_Agente"/>
        <rdfs:comment>Esta clase incluye instituciones o grupos de personas que han obtenido reconocimiento legal como grupo (personas morales). Pueden actuar de manera colectiva como agentes y llevar a cabo acciones y desarrollar procesos, asumiendo la responsabilidad como personas físicas.</rdfs:comment>
        <rdfs:label xml:lang="es">Institución</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E41_Denominacion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E41_Denominacion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E41_Appellation"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E90_Objeto_simbolico"/>
        <rdfs:comment xml:lang="es">Esta clase incluye secuencias de signos que sirven para denominar e identificar entidades de cualquier tipo.</rdfs:comment>
        <rdfs:label xml:lang="es">Denominación</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E42_Identificador -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E42_Identificador">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E42_Identifier"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E41_Denominacion"/>
        <rdfs:comment xml:lang="es">Esta clase incluye cadenas de caracteres o códigos asignados a cualquier bien de interés cultural para identificarlos de manera unívoca y permanente en el contexto de una o más organizaciones. No incluye códigos generados de manera automática para su procesamiento por máquina, salvo que también sean usados por personas.</rdfs:comment>
        <rdfs:label xml:lang="es">Identificador</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E44_Denominacion_de_lugar -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E44_Denominacion_de_lugar">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E44_Place_Appellation"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E41_Denominacion"/>
        <rdfs:comment xml:lang="es">Esta clase recoge identificadores usados para referirse a un lugar.</rdfs:comment>
        <rdfs:label xml:lang="es">Denominación de lugar</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E45_Direccion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E45_Direccion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E45_Address"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E44_Denominacion_de_lugar"/>
        <rdfs:comment xml:lang="es">Esta clase comprende identificadores expresados en forma de sistemas de codificación para lugares, como las direcciones postales.</rdfs:comment>
        <rdfs:label xml:lang="es">Dirección</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E47_Coordenadas_espaciales -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E47_Coordenadas_espaciales">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E47_Spatial_Coordinates"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E44_Denominacion_de_lugar"/>
        <rdfs:comment xml:lang="es">Recoge expresiones alfanuméricas requeridas para localizar instancias de lugar usando un esquema de identificación espacial particular.</rdfs:comment>
        <rdfs:label xml:lang="es">Coordenadas espaciales</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E48_Nombre_lugar -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E48_Nombre_lugar">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E48_Place_Name"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E44_Denominacion_de_lugar"/>
        <rdfs:comment xml:lang="es">Esta clase comprende denominaciones particulares y comunes con las que se identifica un lugar.</rdfs:comment>
        <rdfs:label xml:lang="es">Nombre de lugar</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E49_Denominacion_Tiempo -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E49_Denominacion_Tiempo">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E49_Time_Appellation"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E41_Denominacion"/>
        <rdfs:comment xml:lang="es">Esta clase incluye todas las formas de nombres o códigos usados (estandarizados o no) para referirse a un periodo temporal (como periodos históricos concretos o fechas).</rdfs:comment>
        <rdfs:label xml:lang="es">Denominación de tiempo</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E4_Periodo -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E4_Periodo">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E4_Period"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E2_Entidad_Efimera"/>
        <rdfs:comment xml:lang="es">Clase que recoge fenómenos o manifestaciones culturales que se desarrollan en un espacio y tiempo determinados.</rdfs:comment>
        <rdfs:label xml:lang="es">Periodo</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E50_Fecha -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E50_Fecha">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E50_Date"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E49_Denominacion_Tiempo"/>
        <rdfs:comment xml:lang="es">Recoge formas específicas de identificadores temporales en forma de fechas (estandarizadas o no) con diferentes grados de precisión. Ejemplos: “09/10/2017”; “1546”; “11-ENE-1959”.</rdfs:comment>
        <rdfs:label xml:lang="es">Fecha</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E52_Data -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E52_Data">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E52_Time-Span"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E1_Entidad"/>
        <rdfs:comment xml:lang="es">Esta clase comprende periodos temporales abstractos que tienen un inicio, un fin y una duración específica.</rdfs:comment>
        <rdfs:label xml:lang="es">Data</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E53_Lugar -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E53_Lugar">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E1_Entidad"/>
        <rdfs:comment xml:lang="es">Describe extensiones espaciales independientes de fenómenos temporales o materiales.</rdfs:comment>
        <rdfs:label xml:lang="es">Lugar</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E54_Dimension -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E54_Dimension">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E54_Dimension"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E1_Entidad"/>
        <rdfs:comment xml:lang="es">Medida de una magnitud que caracteriza un objeto.</rdfs:comment>
        <rdfs:label xml:lang="es">Dimensión</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E56_Idioma -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E56_Idioma">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E56_Language"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Tipologia"/>
        <rdfs:comment xml:lang="es">Idioma en el que está expresado un objeto lingüístico determinado.</rdfs:comment>
        <rdfs:label xml:lang="es">Idioma</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E57_Material -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E57_Material">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E57_Material"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Tipologia"/>
        <rdfs:comment xml:lang="es">Esta clase comprende los tipos de materiales utilizados en la producción de un objeto cultural.</rdfs:comment>
        <rdfs:label xml:lang="es">Material</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E5_Evento -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E5_Evento">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E5_Event"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E4_Periodo"/>
        <rdfs:label xml:lang="es">Evento</rdfs:label>
        <rdfs:label xml:lang="es">Incluye cualquier cambio de estado en un sistema cultural, social o físico producido por una serie de fenómenos físicos, culturales, tecnológicos o legales.</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E65_Creacion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E65_Creacion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E65_Creation"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E7_Actividad"/>
        <rdfs:comment xml:lang="es">Esta clase comprende las actividades intelectuales que tiene como fruto un objeto conceptual como leyendas, poemas, leyes, textos, películas, etc.</rdfs:comment>
        <rdfs:label xml:lang="es">Proceso de creación</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E66_Formacion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E66_Formacion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E66_Formation"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Existencia_de_agente"/>
        <rdfs:comment xml:lang="es">Esta clase recoge el contexto en el que un grupo de personas o una institución es creada o fundada.</rdfs:comment>
        <rdfs:label xml:lang="es">Formación</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E67_Nacimiento -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E67_Nacimiento">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E67_Birth"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Existencia_de_agente"/>
        <rdfs:comment xml:lang="es">Esta clase recoge el contexto en el que se produce el nacimiento de una persona.</rdfs:comment>
        <rdfs:label xml:lang="es">Nacimiento</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E68_Disolucion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E68_Disolucion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E68_Dissolution"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Existencia_de_agente"/>
        <rdfs:comment xml:lang="es">Esta clase recoge el contexto en el que se produce la disolución de una institución o entidad corporativa.</rdfs:comment>
        <rdfs:label xml:lang="es">Disolución</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E69_Muerte -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E69_Muerte">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E69_Death"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Existencia_de_agente"/>
        <rdfs:comment xml:lang="es">Esta clase refleja el momento en que deja de existir una persona. Se utiliza exclusivamente para personas.</rdfs:comment>
        <rdfs:label xml:lang="es">Muerte</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E6_Destruccion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E6_Destruccion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E6_Destruction"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E7_Actividad"/>
        <rdfs:comment xml:lang="es">Incluye eventos que tienen que ver con la destrucción, intencionada o no, de objetos culturales.</rdfs:comment>
        <rdfs:label xml:lang="es">Destrucción</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E70_Cosa -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E70_Cosa">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E70_Thing"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E77_Elemento_persistente"/>
        <rdfs:comment xml:lang="es">Se refiere a productos materiales o intelectuales con una cierta estabilidad en el tiempo.</rdfs:comment>
        <rdfs:label xml:lang="es">Cosa</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E73_Objeto_de_Informacion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E73_Objeto_de_Informacion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E73_Information_Object"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E89_Objeto_proposicional"/>
        <rdfs:comment xml:lang="es">Esta clase comprende objetos inmateriales como poemas, imágenes, conjuntos de datos, objetos multimedia, código informático o algoritmos con una estructura identificable. No dependen de un formato específico y pueden existir simultaneamente en más de un formato.</rdfs:comment>
        <rdfs:label xml:lang="es">Objeto de información</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E74_Grupo -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E74_Grupo">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E74_Group"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E39_Agente"/>
        <rdfs:comment xml:lang="es">Incluye cualquier grupo u organización de personas (incluyendo grupos culturales) que actúan de manera colectiva, pero que no se han constituído de manera legal.</rdfs:comment>
        <rdfs:label xml:lang="es">Grupo</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E77_Elemento_persistente -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E77_Elemento_persistente">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E77_Persistent_Item"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E1_Entidad"/>
        <rdfs:comment xml:lang="es"></rdfs:comment>
        <rdfs:comment xml:lang="es">Esta clase comprende todas las entidades físicas o inmateriales que tienen una identidad persistente.</rdfs:comment>
        <rdfs:label xml:lang="es">Entidad persistente</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E78_Coleccion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E78_Coleccion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E78_Collection"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E24_Cosa_fisica_creada_por_humanos"/>
        <rdfs:comment xml:lang="es">Comprende agregaciones de objetos físicos mantenidos por un agente de acuerdo a un plan de desarrollo de colecciones. Equivale a E78_Collection.</rdfs:comment>
        <rdfs:label xml:lang="es">Colección</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E7_Actividad -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E7_Actividad">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E7_Activity"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E5_Evento"/>
        <rdfs:comment xml:lang="es">Acciones desarrolladas por agentes que tienen como resultado cambios de estado en sistemas culturales, sociales o fìsicos.</rdfs:comment>
        <rdfs:label xml:lang="es">Actividad</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E89_Objeto_proposicional -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E89_Objeto_proposicional">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E89_Propositional_Object"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E28_Objeto_Conceptual"/>
        <rdfs:comment xml:lang="es">Elementos materiales o inmateriales que son o representan de alguna manera un conjunto proposiciones sobre cosas reales o imaginarias, y que se documentan como objetos individuales o sirven como materias de discurso.</rdfs:comment>
        <rdfs:label xml:lang="es">Objeto proposicional</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E8_Adquisicion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E8_Adquisicion">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E8_Acquisition"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E7_Actividad"/>
        <rdfs:comment xml:lang="es">Esta clase registra las transferencias de titularidad sobre un objeto cultural.</rdfs:comment>
        <rdfs:label xml:lang="es">Adquisición</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E90_Objeto_simbolico -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E90_Objeto_simbolico">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E90_Symbolic_Object"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E28_Objeto_Conceptual"/>
        <rdfs:comment xml:lang="es">Incluye símbolos o conjuntos de estos (con o sin significado definido) que tienen una estructura definida como caracteres, textos, imágenes, conjuntos de datos, programas informáticos o partituras.</rdfs:comment>
        <rdfs:label xml:lang="es">Objeto simbólico</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#E9_Traslado -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#E9_Traslado">
        <owl:equivalentClass rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E9_Move"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E7_Actividad"/>
        <rdfs:comment xml:lang="es">Esta clase refleja los cambios de ubicación de un objeto físico.</rdfs:comment>
        <rdfs:label xml:lang="es">Traslado</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Esquema_conocimiento -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Esquema_conocimiento">
        <owl:equivalentClass rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E28_Objeto_Conceptual"/>
        <rdfs:comment xml:lang="es">Esta clase todas aquellos esquemas que proporcionan una determinada organización del conocimiento, como pueden ser las taxonomías, los tesauros o las ontologías. Es equivalente a skos:ConceptScheme.</rdfs:comment>
        <rdfs:label xml:lang="es">Esquema de conocimiento</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Estilo_artistico -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Estilo_artistico">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E4_Periodo"/>
        <rdfs:comment xml:lang="es">Recoge instancias de periodos estilísticos o corrientes artísticas específicas en los que se puede englobar la producción de un objeto cultural.</rdfs:comment>
        <rdfs:label xml:lang="es">Estilo artístico</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Existencia_de_agente -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Existencia_de_agente">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E5_Evento"/>
        <rdfs:comment xml:lang="es">Clase que recoge los eventos que dan comienzo o finalizan la existencia de un agente.</rdfs:comment>
        <rdfs:label xml:lang="es">Existencia de agente</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Exposicion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Exposicion">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E7_Actividad"/>
        <rdfs:comment xml:lang="es">Actividad que comprende las tareas curatoriales que permiten exhibir un objeto o colección.</rdfs:comment>
        <rdfs:label xml:lang="es">Exposición</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Modo_de_uso -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Modo_de_uso">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Tipologia"/>
        <rdfs:comment xml:lang="es">Esta clase define la tipología de usos para los que fue creado un objeto cultural.</rdfs:comment>
        <rdfs:label xml:lang="es">Modo de uso</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Nota -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Nota">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E33_Objeto_linguistico"/>
        <rdfs:comment xml:lang="es">Elemento que permite definir facetaciones más o menos extensas sobre un objeto cultural con información no contemplada en otros elementos del modelo.</rdfs:comment>
        <rdfs:label xml:lang="es">Nota</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Performance -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Performance">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E7_Actividad"/>
        <rdfs:comment xml:lang="es">Actividad artística que tiene como principio básico la improvisación y el contacto directo con el espectador. Sólo se consideraran aquellas performances que tienen asociado algún tipo de objeto cultural asociado.</rdfs:comment>
        <rdfs:label xml:lang="es">Performance</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Periodo_temporal -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Periodo_temporal">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E49_Denominacion_Tiempo"/>
        <rdfs:comment xml:lang="es">Esta clase incluye la denominación de periodos de tiempo específicos que tienen correspondencia con una fecha o rango de fechas (como los periodos históricos o los siglos) pero no se expresan como tales.</rdfs:comment>
        <rdfs:label xml:lang="es">Periodo temporal</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Publicacion -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Publicacion">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E7_Actividad"/>
        <rdfs:comment xml:lang="es">Actividad que consiste en la difusión de un documento a través de un medio impreso o digital.</rdfs:comment>
        <rdfs:label xml:lang="es">Publicación</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Rango -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Rango">
        <owl:equivalentClass rdf:resource="https://www.w3.org/TR/owl-time/#time:Interval"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E49_Denominacion_Tiempo"/>
        <rdfs:comment xml:lang="es">Entidad temporal cuyos valores de inicio y final son diferentes. Se puede utilizar cuando no hay certeza de la fecha exacta en que ocurre un evento determinado. Equivalente a time:Interval</rdfs:comment>
        <rdfs:label xml:lang="es">Rango</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Tecnica -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Tecnica">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Tipologia"/>
        <rdfs:comment xml:lang="es">Técnica que se utiliza en la producción o creación del objeto cultural.</rdfs:comment>
        <rdfs:label xml:lang="es">Técnica</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Tipo_caracteristica_fisica -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Tipo_caracteristica_fisica">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Tipologia"/>
        <rdfs:comment xml:lang="es">Esta clase recoge la tipología de características físicas presentes en un objeto y que no están relacionadas con el proceso de producción, restauracción o creación del mismo, sino con agentes externos que han modificado o dañado de alguna manera su estructura original.</rdfs:comment>
        <rdfs:label xml:lang="es">Tipo de característica física</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Tipo_de_objeto_cultural -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Tipo_de_objeto_cultural">
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#Tipologia"/>
        <rdfs:comment xml:lang="es">Esta clase comprende la tipología de objetos culturales que son considerados en el modelo.</rdfs:comment>
        <rdfs:label xml:lang="es">Tipo de objeto cultural</rdfs:label>
    </owl:Class>
    


    <!-- http://mdm/ontologies/2017/9/onto#Tipologia -->

    <owl:Class rdf:about="http://mdm/ontologies/2017/9/onto#Tipologia">
        <owl:equivalentClass rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
        <rdfs:subClassOf rdf:resource="http://mdm/ontologies/2017/9/onto#E28_Objeto_Conceptual"/>
        <rdfs:comment xml:lang="es">Conjunto de ideas, nociones o unidades de pensamiento que sirven para caracterizar la tipología de una entidad determinada. Equivalente a skos:Concept.</rdfs:comment>
        <rdfs:label xml:lang="es">Tipología</rdfs:label>
    </owl:Class>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E11_Modification -->

    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E11_Modification"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E12_Production -->

    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E12_Production"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E18_Physical_Thing -->

    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E18_Physical_Thing"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E21_Person -->

    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E21_Person"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E22_Man-Made_Object -->

    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E22_Man-Made_Object"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E24_Physical_Man-Made_Thing -->

    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E24_Physical_Man-Made_Thing"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E26_Physical_Feature -->

    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E26_Physical_Feature"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E28_Conceptual_Object -->

    <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E28_Conceptual_Object"/>
    


    <!-- http://www.cidoc-crm.org/cidoc-crm/E2_Temporal_Entity -->

